        <link href="Bootstrap/css/bootstrap.css" rel="stylesheet" />
        <link href="Bootstrap/custom.css" rel="stylesheet" />
        <style>
            .rowInCanvas {
                width: 100%;
            }
            .divider {
                width:100%;
                height:5px;
            }
        </style>
    </head>

    <body>
